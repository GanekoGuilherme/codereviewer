{
  "name": "code-reviewer",
  "displayName": "Code Reviewer",
  "description": "",
  "version": "1.0.1",
  "publisher": "guilhermeganeko",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GanekoGuilherme/codereviewer"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-review.start",
        "title": "Start review code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "code-review.start",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "CodeReview",
      "properties": {
        "settings.apiKey": {
          "type": "string",
          "default": "",
          "description": "API KEY gerada no chat GPT (https://platform.openai.com/account/api-keys)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
